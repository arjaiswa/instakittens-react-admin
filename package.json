{
  "name": "instakittens-react-admin",
  "version": "0.1.0",
  "description": "React-admin application for the Instakittens project",
  "author": {
    "name": "Frédéric Bonnet",
    "email": "fredericbonnet@free.fr"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fredericbonnet/instakittens-react-admin.git"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "ra-data-json-server": "^2.4.2",
    "react": "^16.6.3",
    "react-admin": "^2.5.2",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "e2e": "jest -c e2e/jest.config.js",
    "e2e:watch": "jest -c e2e/jest.config.js --watch",
    "cucumber": "cucumber-js",
    "cucumber:watch": "nodemon -e js,feature -x npx cucumber-js --profile watch",
    "cypress:run": "cypress run",
    "cypress:watch": "nodemon -e js,feature -x npm run cypress:run",
    "cypress:open": "cypress open",
    "testcafe": "testcafe -c 4 chrome:headless testcafe/**/*.test.js",
    "testcafe:watch": "testcafe --live chrome testcafe/**/*.test.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-exclude": "^1.0.12",
    "cucumber": "^5.0.3",
    "cypress": "^3.1.5",
    "cypress-cucumber-preprocessor": "^1.9.1",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.7.0",
    "nodemon": "^1.18.9",
    "picklejs": "git+https://github.com/hoverinc/picklejs.git",
    "puppeteer": "^1.11.0",
    "testcafe": "^1.0.0"
  }
}
